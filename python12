Sno=1
Sname="Surya"
Group="BCA"
S1=int(input())
S2=int(input())
S3=int(input())
Tot=S1+S2+S3
Avg=(S1+S2+S3)/3
if Avg>=90:
    print("O grade")
elif Avg>=80:
    print("A grade")
elif Avg>=70:
    print("B grade")
elif Avg>=60:
    print("C grade")
elif Avg>=50:
    print("D grade")
else:
    print("Pass")
